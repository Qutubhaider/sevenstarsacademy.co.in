<%@ Control Language="C#" AutoEventWireup="true" CodeFile="counters.ascx.cs" Inherits="control_counters" %>
<div class="counter-area counter-bg pd overlay-black">
         <div class="container">
            <div class="row">
               <div class="col-md-3 col-sm-6">
                  <div class="counter-item text-center">
                     <i class="fa fa-book"></i>
                     <span class="count">6000</span>
                     Books
                  </div>
                  </div>
               <div class="col-md-3 col-sm-6">
                  <div class="counter-item text-center">
                     <i class="fa fa-users"></i>
                     <span class="count">350</span>
                     Students every session
                  </div>
               </div>
               <div class="col-md-3 col-sm-6">
                  <div class="counter-item text-center">
                     <i class="fa fa-bullhorn"></i>
                     <span class="count">100</span>
                    Percent Result
                  </div>
               </div>
               <div class="col-md-3 col-sm-6">
                  <div class="counter-item text-center">
                     <i class="fa fa-line-chart"></i>
                     <span >A</span>
                     Grade
                  </div>
               </div>
            </div>
         </div>
      </div>